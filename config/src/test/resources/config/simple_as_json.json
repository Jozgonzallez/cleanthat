{
  "syntax_version" : "2021-08-02",
  "meta" : {
    "labels" : [ "cleanthat" ],
    "refs" : {
      "protected_patterns" : [ "master" ]
    }
  },
  "source_code" : {
    "includes" : [ "regex:.*\\.java" ],
    "excludes" : [ "regex:.*/generated/.*" ],
    "encoding" : "UTF-8"
  },
  "languages" : [ {
    "language" : "java",
    "language_version" : "11",
    "processors" : [ {
      "engine" : "revelc_imports",
      "parameters" : {
        "remove_unused" : true,
        "groups" : "java.,javax.,org.,com.",
        "static_groups" : "java,*"
      }
    } ]
  } ]
}